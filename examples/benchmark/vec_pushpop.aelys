needs std.io
needs std.convert
needs std.time

let n = 1000000
io.print("Vec push/pop benchmark (n=" + convert.to_string(n) + ")")

let v = Vec[]

let t1 = time.timer()
let mut i = 0
while i < n {
    v.push(i)
    i = i + 1
}
let push_time = time.elapsed_ms(t1)
io.print("Push time: " + convert.to_string(push_time) + "ms")
io.print("Final len: " + convert.to_string(v.len()))

let t2 = time.timer()
while v.len() > 0 {
    v.pop()
}
let pop_time = time.elapsed_ms(t2)
io.print("Pop time: " + convert.to_string(pop_time) + "ms")
io.print("Final len: " + convert.to_string(v.len()))
